public class Multiplication {

    public static void main(String[] args) {

      
        int rows = 10;
        int cols = 10;

        
        int table[][] = new int[rows][cols];

        for (int i = 0; i < rows; i++) {
            for (int j = 0; j < cols; j++) {
              
                table[i][j] = (i + 1) * (j + 2);

              
                System.out.print(table[i][j] + "\t");
            }
           
            System.out.println();
        }
    }
}
